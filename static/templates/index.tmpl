	<section class="charges-widget">
		<h2>Charges</h2>
		<div class="carousel-wrapper">
			<button id="charges-prev" class="carousel-btn" aria-label="Charge anterior">&#x2039;</button>
			<!-- O conteúdo aqui será preenchido dinamicamente -->
			<div class="charges-carousel" id="charges-carousel"></div>
			<button id="charges-next" class="carousel-btn" aria-label="Próxima charge">&#x203A;</button>
		</div>
	</section>

	<script>
		document.addEventListener('DOMContentLoaded', () => {
			
			// Função principal que inicia todo o processo
			function inicializarCarrosselDeCharges() {
				const carouselContainer = document.getElementById('charges-carousel');
				if (!carouselContainer) return;

				fetch('/api/charges')
					.then(response => {
						if (!response.ok) throw new Error('Falha ao buscar charges');
						return response.json();
					})
					.then(charges => {
						if (!charges || charges.length === 0) {
							carouselContainer.innerHTML = '<p>Nenhuma charge encontrada.</p>';
							return;
						}
						// Preenche o carrossel com os dados da API
						charges.forEach(charge => {
							const chargeLink = document.createElement('a');
							chargeLink.href = `/charge/${charge.id}`;
							chargeLink.classList.add('charge-item');
							chargeLink.innerHTML = `<img src="${charge.url}" alt="${charge.title}" loading="lazy">`;
							carouselContainer.appendChild(chargeLink);
						});
						
						// Só depois de preencher, ativa a lógica de movimento
						ativarLogicaCarrosselInfinito();
					})
					.catch(error => {
						console.error('Erro no carrossel de charges:', error);
						carouselContainer.innerHTML = '<p>Erro ao carregar as charges.</p>';
					});
			}

			// Função que contém a lógica de movimento, clones e eventos
			function ativarLogicaCarrosselInfinito() {
				const carousel = document.getElementById('charges-carousel');
				const prevBtn = document.getElementById('charges-prev');
				const nextBtn = document.getElementById('charges-next');
				if (!carousel || !prevBtn || !nextBtn) return;
				
				const originalItems = Array.from(carousel.querySelectorAll('.charge-item'));
				const originalCount = originalItems.length;
				if (originalCount === 0) return;

				// Define quantos itens são visíveis (para responsividade no futuro)
				const getVisibleCount = () => {
					if (window.innerWidth <= 500) return 1;
					if (window.innerWidth <= 768) return 2;
					return 3;
				};
				
				// Clona os itens para criar o efeito infinito
				originalItems.forEach(item => carousel.appendChild(item.cloneNode(true)));
				originalItems.slice().reverse().forEach(item => carousel.prepend(item.cloneNode(true)));

				let itemWidth = originalItems[0].offsetWidth;
				let currentIndex = originalCount; // Começa no primeiro item real
				let isTransitioning = false;

				const setCarouselPosition = () => {
					carousel.style.transition = 'none';
					carousel.style.transform = `translateX(${-currentIndex * itemWidth}px)`;
				};

				const slide = (direction) => {
					if (isTransitioning) return;
					isTransitioning = true;
					
					currentIndex += direction;
					carousel.style.transition = 'transform 0.45s ease-in-out';
					carousel.style.transform = `translateX(${-currentIndex * itemWidth}px)`;
				};
				
				// Evento que "reseta" a posição quando chega nos clones
				carousel.addEventListener('transitionend', () => {
					isTransitioning = false;
					if (currentIndex >= originalCount * 2) {
						currentIndex = originalCount;
						setCarouselPosition();
					}
					if (currentIndex < originalCount) {
						currentIndex = originalCount * 2 -1;
						setCarouselPosition();
					}
				});

				// Eventos dos botões
				nextBtn.addEventListener('click', () => slide(1));
				prevBtn.addEventListener('click', () => slide(-1));
				
				// Ajusta o tamanho em caso de redimensionamento da janela
				window.addEventListener('resize', () => {
					itemWidth = originalItems[0].offsetWidth;
					setCarouselPosition();
				});

				// Define a posição inicial
				setCarouselPosition();
			}

			// Inicia todo o processo
			inicializarCarrosselDeCharges();
		});
	</script>
