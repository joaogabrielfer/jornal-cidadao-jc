	<script>
		document.addEventListener('DOMContentLoaded', () => {
			const carouselContainer = document.getElementById('charges-carousel');
			if (!carouselContainer) return;

			// 1. Limpa as charges estáticas do HTML
			carouselContainer.innerHTML = '';

			// 2. Busca as charges da sua API
			fetch('/api/charges')
				.then(response => response.json())
				.then(charges => {
					if (!charges || charges.length === 0) {
						carouselContainer.innerHTML = '<p>Nenhuma charge encontrada.</p>';
						return;
					}

					// 3. Para cada charge, cria o elemento HTML e o adiciona ao carrossel
					charges.forEach(charge => {
						const chargeLink = document.createElement('a');
						chargeLink.href = `/charge/${charge.id}`; // URL correta para o seu backend
						chargeLink.classList.add('charge-item');

						const chargeImage = document.createElement('img');
						chargeImage.src = charge.url; // A URL já vem pronta da sua API
						chargeImage.alt = charge.title;
						chargeImage.loading = 'lazy';

						chargeLink.appendChild(chargeImage);
						carouselContainer.appendChild(chargeLink);
					});

					// IMPORTANTE: Se o seu script de carrossel (o que você já tem)
					// precisar ser reiniciado depois de adicionar os itens,
					// chame a função de inicialização dele aqui.
				})
				.catch(error => {
					console.error('Erro ao carregar as charges:', error);
					carouselContainer.innerHTML = '<p>Não foi possível carregar as charges.</p>';
				});
		});
	</script>
