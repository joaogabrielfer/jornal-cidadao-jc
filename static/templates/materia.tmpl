    // Reseta a largura para 0 antes de animar (para o caso de "voltar")
    document.getElementById('fillA').style.width = '0%';
    document.getElementById('fillB').style.width = '0%';
    document.getElementById('fillC').style.width = '0%';

    // Adiciona um pequeno atraso para a animação de "fill" funcionar
    setTimeout(() => {
      document.getElementById('fillA').style.width = data.A + '%';
      document.getElementById('fillB').style.width = data.B + '%';
      document.getElementById('fillC').style.width = data.C + '%';
    }, 100); // 100ms de atraso
  }

  // --- Lógica Inicial ---
  function initializePollWidget() {
    if (!pollWidgetBox) return;

    if (!isLoggedIn) {
      showState(pollStateNotLoggedIn);
    } else if (hasVoted) {
      updateResultsUI(pollResultsData);
      showState(pollStateResults);
    } else {
      showState(pollStateVoting);
    }
  }

  // --- Event Listeners ---

  // Habilita o botão de votar se uma opção for selecionada
  if (pollForm) {
    pollForm.addEventListener('change', () => {
      if (voteButton) {
        voteButton.disabled = !pollForm.querySelector('input[name="poll-answer"]:checked');
      }
    });

    // Simula o envio do voto
    pollForm.addEventListener('submit', (e) => {
      e.preventDefault();
      const selectedOption = pollForm.querySelector('input[name="poll-answer"]:checked');
      
      if (selectedOption) {
        console.log("Voto enviado para:", selectedOption.value);
        // Em um app real, você enviaria o voto para o backend aqui
        // e receberia os resultados atualizados.

        hasVoted = true;
        // Para este exemplo, apenas mostramos os resultados simulados
        updateResultsUI(pollResultsData);
        showState(pollStateResults);
      }
    });
  }

  // Botão "Voltar enquete"
  if (backButton) {
    backButton.addEventListener('click', () => {
      // Para demonstração, volta ao estado de votação
      // (Resetar o 'hasVoted' permite votar de novo para teste)
      if (isLoggedIn) {
        hasVoted = false; 
        if (voteButton) voteButton.disabled = true; // Reseta o botão
        pollForm.reset(); // Limpa a seleção
        showState(pollStateVoting);
      } else {
        showState(pollStateNotLoggedIn);
      }
    });
  }

  // Simula o login ao clicar no link (APENAS PARA TESTE)
  if (loginLink) {
    loginLink.addEventListener('click', (e) => {
      e.preventDefault(); // Impede de ir para a página /login
      alert("Simulando login... Você agora está logado.");
      isLoggedIn = true;
      initializePollWidget(); // Re-inicia o widget no estado logado
    });
  }

  // Inicializa o widget
  initializePollWidget();
});
</script>




                    <p class="texto">O governo do Rio de Janeiro confirmou, nesta terça-feira (28), que ao menos <span class="highlight">64 pessoas morreram durante a Operação Contenção</span>, deflagrada nas comunidades dos complexos da Penha e do Alemão.</p>
